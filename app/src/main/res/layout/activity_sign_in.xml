<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorBlue"
    tools:context=".MainActivity">

    <ImageView
        android:id="@+id/whiteBox"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginStart="30dp"
        android:layout_marginTop="232dp"
        android:layout_marginEnd="30dp"
        android:layout_marginBottom="202dp"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/login_path" />

    <com.facebook.login.widget.LoginButton
        android:id="@+id/fb"
        android:layout_width="60dp"
        android:layout_height="58dp"
        android:layout_marginStart="84.3dp"
        android:layout_marginEnd="3dp"
        android:layout_marginBottom="58dp"
        android:textSize="45dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/tt"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/tt"
        style="?android:attr/borderlessButtonStyle"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginStart="153dp"
        android:layout_marginEnd="153dp"
        android:layout_marginBottom="57dp"
        android:background="@drawable/twitter"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/twitter" />

    <Button
        android:id="@+id/gg"
        style="?android:attr/borderlessButtonStyle"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginStart="2dp"
        android:layout_marginEnd="84.3dp"
        android:layout_marginBottom="57dp"
        android:background="@drawable/gg"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/tt"
        app:srcCompat="@drawable/gg" />

    <Button
        android:id="@+id/anonymous"
        style="?android:attr/borderlessButtonStyle"
        android:layout_width="53.3dp"
        android:layout_height="13.7dp"
        android:layout_marginStart="153dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="153dp"
        android:layout_marginBottom="33dp"
        android:background="@drawable/guest_login"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tt"
        app:srcCompat="@drawable/guest_login" />

    <Button
        android:id="@+id/check"
        style="?android:attr/borderlessButtonStyle"
        android:layout_width="53.3dp"
        android:layout_height="13.7dp"
        android:layout_marginStart="117dp"
        android:layout_marginTop="436dp"
        android:layout_marginEnd="10dp"
        android:layout_marginBottom="61dp"
        android:background="@drawable/find_password"
        app:layout_constraintBottom_toTopOf="@+id/imageView20"
        app:layout_constraintEnd_toStartOf="@+id/imageViewdv"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/signup" />

    <Button
        android:id="@+id/signup"
        style="?android:attr/borderlessButtonStyle"
        android:layout_width="51.3dp"
        android:layout_height="13.7dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="436dp"
        android:layout_marginEnd="117dp"
        android:layout_marginBottom="61dp"
        android:background="@drawable/signup"
        app:layout_constraintBottom_toTopOf="@+id/imageView20"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imageViewdv"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/find_password" />

    <Button
        android:id="@+id/Signin"
        style="?android:attr/borderlessButtonStyle"
        android:layout_width="26dp"
        android:layout_height="13.7dp"
        android:layout_marginStart="167dp"
        android:layout_marginTop="170dp"
        android:layout_marginEnd="167dp"
        android:layout_marginBottom="20dp"
        android:background="@drawable/login"
        app:layout_constraintBottom_toBottomOf="@+id/whiteBox"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/whiteBox"
        app:srcCompat="@drawable/login" />

    <EditText
        android:id="@+id/signin_email"
        android:layout_width="238.3dp"
        android:layout_height="38dp"
        android:layout_marginStart="60dp"
        android:layout_marginTop="90dp"
        android:layout_marginEnd="60dp"
        android:layout_marginBottom="17dp"
        android:backgroundTint="#bce0fd"
        android:ems="20"
        android:hint="이메일 주소"
        android:inputType="textEmailAddress"
        android:textColor="#555555"
        android:textColorHint="#999999"
        android:textSize="13.3sp"
        app:layout_constraintBottom_toTopOf="@+id/signIn_pw"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/whiteBox" />

    <EditText
        android:id="@+id/signIn_pw"
        android:layout_width="238.3dp"
        android:layout_height="38dp"
        android:layout_marginStart="60dp"
        android:layout_marginEnd="60dp"
        android:layout_marginBottom="11dp"
        android:backgroundTint="#bce0fd"
        android:ems="20"
        android:hint="비밀번호"
        android:inputType="textPassword"
        android:textColor="#555555"
        android:textColorHint="#999999"
        android:textSize="13.3sp"
        app:layout_constraintBottom_toTopOf="@+id/Signin"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.509"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/welcome"
        android:layout_width="99.3dp"
        android:layout_height="29.7dp"
        android:layout_marginTop="26dp"
        android:background="@null"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/whiteBox"
        app:srcCompat="@drawable/welcome" />

    <ImageView
        android:id="@+id/imageView19"
        android:layout_width="216.7dp"
        android:layout_height="12dp"
        android:layout_marginStart="72dp"
        android:layout_marginEnd="72dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/welcome"
        app:srcCompat="@drawable/carrying_info" />

    <ImageView
        android:id="@+id/imageView20"
        android:layout_width="165.3dp"
        android:layout_height="13.7dp"
        android:layout_marginStart="97dp"
        android:layout_marginTop="73dp"
        android:layout_marginEnd="97dp"
        android:layout_marginBottom="10dp"
        app:layout_constraintBottom_toTopOf="@+id/tt"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/whiteBox"
        app:srcCompat="@drawable/sns_info" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="189.3dp"
        android:layout_height="189.3dp"
        android:layout_marginStart="105.7dp"
        android:layout_marginTop="33dp"
        android:layout_marginEnd="105.7dp"
        android:layout_marginBottom="11dp"
        app:layout_constraintBottom_toTopOf="@+id/whiteBox"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:scaleType="fitXY"
        app:srcCompat="@drawable/carrying" />

    <ImageView
        android:id="@+id/imageViewdv"
        android:layout_width="0.3dp"
        android:layout_height="8.3dp"
        android:layout_marginTop="438dp"
        android:layout_marginBottom="63dp"
        app:layout_constraintBottom_toTopOf="@+id/imageView20"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/devide_line" />

</androidx.constraintlayout.widget.ConstraintLayout>